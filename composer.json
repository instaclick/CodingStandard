{
    "name": "instaclick/coding-standard",
    "description": "Instaclick coding standard for PHP_CodeSniffer",
    "license": "MIT",
    "authors": [
        {
            "name": "Instaclick Inc.",
            "homepage": "http://instaclick.com/"
        }
    ],
    "target-dir": "Instaclick",
    "autoload": {
        "classmap": [
            "Instaclick/Sniffs/"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:instaclick/ObjectCalisthenicsSniffs.git"
        }
    ],
    "require": {
        "squizlabs/php_codesniffer": "~1.5",
        "instaclick/symfony2-coding-standard": "~0.0.2",
        "instaclick/object-calisthenics-sniffs": "~1.0.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
